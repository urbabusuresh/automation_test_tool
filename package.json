{
  "name": "autotestflow",
  "version": "1.0.0",
  "description": "No-code test automation builder that records, generates, and replays test cases",
  "main": "index.js",
  "scripts": {
    "install-all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "backend": "cd backend && npm start",
    "frontend": "cd frontend && npm start",
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "build": "cd frontend && npm run build",
    "test": "cd backend && npm test"
  },
  "keywords": [
    "automation",
    "testing",
    "no-code",
    "playwright",
    "selenium",
    "test-recorder"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
